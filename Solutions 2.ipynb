    "# Exercise 10.c\n",
    "\n",
    "# solution with flags\n",
    "\n",
    "def overlap(lst1, lst2):\n",
    "    ans = []\n",
    "    span = []\n",
    "    flag = False\n",
    "\n",
    "    for i1, i2 in zip(lst1, lst2):\n",
    "        if i1 == i2:\n",
    "            span.append(i1)\n",
    "            flag = True\n",
    "        else:\n",
    "            if flag:\n",
    "                ans.append(span)\n",
    "                span = []\n",
    "                flag = False\n",
    "    \n",
    "    ans.append(span)\n",
    "    \n",
    "    return ans\n",
    "    \n",
    "    return sorted(ans, reverse=True, key=len)[0]\n",
    "\n",
    "print(overlap([1, 2, 3, 4, 5, 6], [0, 2, 2, 4, 5, 6, 1, 3])) # >>> [[2], [4, 5, 6]]\n",
    "print(overlap([1, 2, 3], [2, 3, 4])) # >>> [[]]"
   ]
