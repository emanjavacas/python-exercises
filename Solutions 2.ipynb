    "# Exercise 10.c\n",
    "\n",
    "# solution without flags\n",
    "\n",
    "def overlap(lst1, lst2):\n",
    "    span = []\n",
    "    output = []\n",
    "    \n",
    "    for x, y in zip(lst1, lst2):\n",
    "        if x == y:\n",
    "            span.append(x)\n",
    "        else:\n",
    "            if len(span) != 0:\n",
    "                output.append(span)\n",
    "                span = []\n",
    "                \n",
    "    output.append(span)\n",
    "    \n",
    "    return output\n",
    "\n",
    "print(overlap([1, 2, 3, 4, 5, 6], [0, 2, 2, 4, 5, 6, 1, 3])) # >>> [2], [4, 5, 6]]\n",
    "print(overlap([1, 2, 3], [2, 3, 4])) # >>> [[]]"
   ]
