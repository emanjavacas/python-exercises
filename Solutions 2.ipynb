    "# Exercise 11\n",
    "\n",
    "def ngram(inp, n):\n",
    "    ans = []\n",
    "    for i in range(len(inp)):\n",
    "        if i + n > len(inp):\n",
    "            break\n",
    "        ans.append(inp[i:i+n])\n",
    "    return ans\n",
    "\n",
    "\n",
    "ngram(['The', 'brown', 'fox', 'jumped', 'over', 'the', 'lazy', 'dog'], 3)"
